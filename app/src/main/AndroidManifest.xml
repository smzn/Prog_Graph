<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mizuno.prog_graph" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".GraphActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CircleActivity"
            android:label="@string/title_activity_circle" >
        </activity>
        <activity
            android:name=".BarActivity"
            android:label="@string/title_activity_bar" >
        </activity>
        <activity
            android:name=".LineActivity"
            android:label="@string/title_activity_line" >
        </activity>
    </application>

</manifest>
